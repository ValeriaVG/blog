{
  "name": "valeriavgblog",
  "description": "ValeriaVG blog",
  "version": "1.0.0",
  "author": "Valeria Viana Gusmao <valeria.viana.gusmao@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.202",
    "gatsby-link": "^1.6.37",
    "gatsby-plugin-react-helmet": "^2.0.5",
    "surge": "^0.19.0"
  },
  "keywords": [
    "valeriavg",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "jest --watchAll",
    "test:ci": "jest --ci"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/public/",
      "/.cache/"
    ],
    "transform": {
      "\\.js$": "./transformer.js"
    },
    "setupFiles": [
      "<rootDir>/testsSetup.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-jest": "^22.2.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.3.0",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0"
  }
}
